@model Octopus.Models.Cartera

@{
    ViewData["Title"] = "Details";
}



<div class="col-12 form-inline" style="border-radius:5px;">
  
   <input type="hidden" id="comisionTAE" asp-for="@Model.ComisionTAE" />
   <div class="main row">
       

       <div class="col-12 col-sm-6 col-md-6 col-lg-3 form-group">
           <label class="col-6 col-sm-6 col-md-12 col-lg-12">
               TAE
           </label>
           <a class="col-6 col-sm-6 col-md-12 col-lg-12 text-center form-control text-dark" >$@String.Format("{0:0.00}", @Model.SaldoTAE)</a>

       </div>
       <div class="col-12 col-sm-6 col-md-6 col-lg-3 form-group">
           <label class="col-6 col-sm-6 col-md-12 col-lg-12 ">
               Comisi√≥n
           </label>
           <a id="mySaldoTAE" class="col-6 col-sm-6 col-md-12 col-lg-12 text-center text-dark form-control">@Model.ComisionTAE %</a>
       </div>
       <div class="col-12 col-sm-6 col-md-6 col-lg-3 form-group">
           <label class="col-6 col-sm-6 col-md-12 col-lg-12 ">
               Venta
           </label>
           <a id="asignado" class="col-6 col-sm-6 col-md-12 col-lg-12 text-center text-dark form-control">$@Model.Venta</a>
       </div>
       <div class="col-12 col-sm-6 col-md-6 col-lg-3 form-group">
           <label class="col-6 col-sm-6 col-md-12 col-lg-12 ">
               Saldo
           </label>
           <a class="col-6 col-sm-6 col-md-12 col-lg-12 text-center form-control text-dark" id="mySaldoGlobal form-control">$@String.Format("{0:0.00}", @Model.SaldoNormal)</a>

       </div>





   </div>
</div>

